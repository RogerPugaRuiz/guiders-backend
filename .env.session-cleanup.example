# Configuración del sistema automático de limpieza de sesiones

# Habilitar/deshabilitar el sistema de limpieza automática
# SESSION_CLEANUP_ENABLED=true

# Tamaño del lote para procesar visitantes en cada ejecución
# SESSION_CLEANUP_BATCH_SIZE=100

# Configuraciones avanzadas de timeouts para diferentes tipos de sesiones
# SESSION_TIMEOUT_SHORT_MS=300000     # 5 minutos
# SESSION_TIMEOUT_MEDIUM_MS=900000    # 15 minutos  
# SESSION_TIMEOUT_LONG_MS=1800000     # 30 minutos
# SESSION_TIMEOUT_EXTENDED_MS=3600000 # 60 minutos

# Configuración de logging para debugging
# SESSION_CLEANUP_DEBUG=false
# SESSION_CLEANUP_METRICS=true